{
  "name": "bruno-test-project",
  "version": "1.0.0",
  "description": "Projet de test complet pour Bruno avec variables d'environnement, collections, et fonctions JavaScript partag√©es",
  "main": "scripts/global-setup.js",
  "scripts": {
    "bruno": "npx @usebruno/cli",
    "test": "npx @usebruno/cli run bruno-collections -r --env Development",
    "test:ci": "npx @usebruno/cli run bruno-collections -r --env Development --reporter-html results.html",
    "test:ci-only-standard": "npx @usebruno/cli run bruno-collections/auth-api -r --env Development --reporter-html ci-results.html",
    "test:staging": "npx @usebruno/cli run bruno-collections -r --env Staging",
    "test:prod": "npx @usebruno/cli run bruno-collections -r --env Production",
    "test:auth": "npx @usebruno/cli run bruno-collections/auth-api/Login.bru -r --env Development",
    "test:users": "npx @usebruno/cli run bruno-collections/users-api --env Development",
    "test:products": "npx @usebruno/cli run bruno-collections/products-api --env Development",
    "test:data-csv": "npx @usebruno/cli run bruno-collections/data-driven-api/CreateUsersFromCSV.bru --env Development --csv-file-path data/users.csv",
    "test:data-json": "npx @usebruno/cli run bruno-collections/data-driven-api/CreateProductsFromJSON.bru --env Development --json-file-path data/products.json",
    "test:scenarios": "npx @usebruno/cli run bruno-collections/data-driven-api/LoginScenariosFromJSON.bru --env Development --json-file-path data/test-scenarios.json",
    "test:data-all": "npx @usebruno/cli run bruno-collections/data-driven-api -r --env Development",
    "test:report": "npx @usebruno/cli run bruno-collections -r --env Development --reporter-html report.html",
    "test:junit": "npx @usebruno/cli run bruno-collections -r --env Development --reporter-junit junit.xml",
    "test:json": "npx @usebruno/cli run bruno-collections -r --env Development --reporter-json report.json"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@usebruno/cli": "^1.0.0"
  },
  "keywords": [
    "bruno",
    "api-testing",
    "javascript",
    "environments",
    "collections"
  ],
  "author": "Test User",
  "license": "MIT",
  "bruno": {
    "version": "1",
    "name": "Bruno Test Project",
    "type": "collection",
    "environments": [
      {
        "name": "Development",
        "file": "./environments/Development.bru"
      },
      {
        "name": "Staging",
        "file": "./environments/Staging.bru"
      },
      {
        "name": "Production",
        "file": "./environments/Production.bru"
      }
    ]
  }
}
